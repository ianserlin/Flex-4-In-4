<script type="syntaxhighlighter" class="brush: as3"><![CDATA[
  // trace out a single line of text
	trace( "this all shows up on one line, with a line break at the end" );

	// trace out a few things concatenated onto one line with spaces in-between
	// and a line break at the end	
	var a:String 	= "Flex";
	var b:int 		=	4;
	var c:String 	= "in";
	var d:String	= "4";
	trace( a, b, c, d ); // traces out "Flex 4 in 4"
	
	// trace out a random object
  trace( this ); // traces out the name of whatever the current this object is
</script>